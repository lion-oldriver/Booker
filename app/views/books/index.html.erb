<h1>Books</h1>
<table>
  <div class="headline">
    <div>Title</div>
    <div>Body</div>
    <div></div>
  </div>
  <div class="table-body">
    <% @books.each do |book| %>
      <tr>
        <td><%= link_to book.title,book_path(book) %></td>
        <td><%= book.body %></td>
      </tr>
    <% end %>
  </div>
</table>
